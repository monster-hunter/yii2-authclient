{
    "name": "zacksleo/yii2-authclient",
    "description": "Yii AuthClient for LianLuo",
    "keywords": ["yii2", "OAuth", "OpenID", "auth", "api"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/zacksleo/yii2-authclient"
    },
    "authors": [
        {
            "name": "zacksleo",
            "email": "zacks.leo@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2-authclient": "^2.1"
    },
    "require-dev":{
        "phpunit/phpunit": "4.8.34"
    },
    "config": {
        "process-timeout": 1800,
        "vendor-dir": "vendor",
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "zacksleo\\yii2\\authclient\\": "src",
            "zacksleo\\yii2\\authclient\\tests\\": "tests"
        }
    }
}
